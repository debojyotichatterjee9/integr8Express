{
  "services": {
    "rest": {
      "host": "localhost",
      "port": 3000
    }
  },
  "cluster": {
    "enabled": true,
    "maxRestarts": 5,
    "restartWindow": 60000,
    "shutdownTimeout": 30000,
    "workerReadyTimeout": 10000
  },
  "logging": {
    "level": "info",
    "fileLogging": true
  },
  "workerThreads": {
    "poolSize": 4,
    "taskTimeout": 30000
  },
  "database": {
    "mongo_uri": "mongodb://localhost:27017/your_database"
  }
}